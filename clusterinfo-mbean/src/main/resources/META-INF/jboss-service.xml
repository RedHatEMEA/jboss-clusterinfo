<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server
    PUBLIC "-//JBoss//DTD MBean Service 4.2//EN"
    "http://www.jboss.org/j2ee/dtd/jboss-service_5_0.dtd">
<server>
	<mbean code="org.jboss.clusterinfo.mbean.ClusterInfo" name="clusterinfo:service=ClusterInfo"
		xmbean-dd="META-INF/clusterinfo-xmbean.xml">
		<attribute name="TreeCacheMBeanServiceLocator" replace="true"
			trim="true">
			jboss.cache:service=clusterinfoMBeanSessionCache
		</attribute>
	</mbean>
</server>